	<servlet>
		<servlet-name>cleanup.jsp</servlet-name>
		<jsp-file>/cleanup.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>cleanup.jsp</servlet-name>
		<url-pattern>/cleanup</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ValidatorService</display-name>
		<servlet-name>ValidatorService</servlet-name>
		<servlet-class>org.validator.servlets.ValidatorService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ValidatorService</servlet-name>
		<url-pattern>/results</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>405</error-code>
		<location>/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/error.jsp</location>
	</error-page>
</web-app>
